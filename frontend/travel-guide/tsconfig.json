{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/users/edit/page.jsx", "src/components/Table/Table/TableUser.jsx", "src/app/users/page.jsx", "src/app/users/new/page.jsx", "src/hooks/useUser.jsx", "src/components/Table/TableFooter/TableFooter.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
